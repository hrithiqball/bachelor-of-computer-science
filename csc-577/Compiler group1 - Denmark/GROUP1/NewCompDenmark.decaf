klasse NewCompDenmark
{
	offentlig statisk ugyldig vigtigste (Snor [] args)
	{
		flyde X1,Y1;
		int I,J,K;
		X1=0;
		Y1=10;
		K=10;
		J=-10;
		
		til (I=1;I+10<=K;I=I-1)
		{
			mens (K+I<20)
			{
				J=I;
				
				gor
				{
					hvis (X1>Y1)
					J=J-2;
						andet
						J=J/4;
					
					X1=X1*2.0;
				}
				mens (J>=20);
				
				K=K+3;
			}
		}
	}
}