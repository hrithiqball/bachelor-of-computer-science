klas NewCompDutch
{
	openbaar statisch leegte voornaamst (Snaar [] args)
	{
		vlot X1,Y1;
		int I,J,K;
		X1=0;
		Y1=10;
		K=10;
		J=-10;
		
		voor (I=1;I+10<=K;I=I-1)
		{
			terwijl (K+I<20)
			{
				J=I;
				
				Doen
				{
					als (X1>Y1)
					J=J-2;
						anders
						J=J/4;
					
					X1=X1*2.0;
				}
				terwijl (J>=20);
				
				K=K+3;
			}
		}
	}
}